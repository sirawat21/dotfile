#################### PATH ####################
# HOME BREW
HOME_BREW=/opt/homebrew/bin
# GLOBAL PATH
export PATH=$PATH:$HOME_BREW
#################### ALIAS ####################
# Git
alias git="$HOME_BREW/git"
# Python3
alias python="$HOME_BREW/python3"
alias pip="$HOME_BREW/pip3"
# Neo Vim
alias vim="$HOME_BREW/nvim"
# Command ls
alias ls='ls -G'
alias la='ls -aG'
alias ll='ls -lG'
alias lla='ls -laG'
alias lal='ls -laG'