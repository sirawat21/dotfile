###### [2]PROFILE ######

### ALIAS ###
# ls
alias ls="ls -G"
alias la="ls -aG"
alias ll="ls -lG"
alias lla="ls -laG"
alias lal="ls -laG"
### ALIAS REPLACED CMD  ###
# Zsh
alias zsh="$HOMEBREW/zsh"

# Neo Vim
alias vim="$HOMEBREW/nvim"
alias vi="$HOMEBREW/nvim"

# Git
alias git="$HOMEBREW/git"

# Python version management setup
eval "$(pyenv init --no-rehash -)"
eval "$(pyenv virtualenv-init -)"

